  //@UI.defaultValue : #( 'ELEMENT_OF_REFERENCED_ENTITY: CourseDuration' )
  CourseDuration : abap.numc(4);
  @UI.defaultValue : 'X' //#( 'ELEMENT_OF_REFERENCED_ENTITY: Status' )
  Status         : abap_boolean;
}
